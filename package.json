{
  "name": "@mjparkgh/pull-stream",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.8.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --tsconfig tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "dev": "tsup --watch",
    "lint": "prettier \"{src,tests}/**/*.ts\" --write --log-level silent && eslint src tests --ext .ts --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "es-toolkit": "^1.39.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/node": "^20.19.7",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.1.0"
  }
}
